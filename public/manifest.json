{
    "name": "XFS Wallet",
    "description": "XFS Wallet",
    "version": "1.2.0",
    "manifest_version": 3,
    "icons": { "16": "icon16.png",
      "48": "icon48.png",
     "128": "icon128.png" },
    "action": {
      "default_popup": "popup.html",
      "default_icon": { 
        "16": "icon16.png",
        "24": "icon24.png",
        "32": "icon32.png"
      }
    },
    "permissions": ["tabs","activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["injectscript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["contentscript.js"],
        "matches": ["<all_urls>"]
      }
    ]
}